{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb"],
    "plugins":["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["webpack.config.js", "**/dist/*.js","*.json"],
    "rules": {
        "nonblock-statement-body-position":"off",
        "no-param-reassign":"off"
    }
}
